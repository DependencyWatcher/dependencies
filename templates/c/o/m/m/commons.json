{
  "pages": {
    "download": "http://commons.apache.org/proper/${NAME}/download_${SHORT_LC_NAME}.cgi", 
    "docs": "http://commons.apache.org/${SHORT_LC_NAME}/javadocs/api-${VERSION}/index.html", 
    "changelist": "http://commons.apache.org/proper/${NAME}/changes-report.html", 
    "issues": "http://issues.apache.org/jira/browse/${SHORT_UC_NAME}", 
    "home": "http://commons.apache.org/proper/${NAME}"
  }, 
  "detectors": {
    "changelist": {
      "xpath": {
        "url": "http://commons.apache.org/proper/${NAME}/changes-report.html", 
        "xpath": "//a[@name='a${VERSION}']/following-sibling::table"
      }
    }, 
    "version": {
      "xpath": {
        "url": "http://central.maven.org/maven2/${MVN_PATH}/maven-metadata.xml", 
        "xpath": "/metadata/versioning/release/text()|/metadata/version/text()"
      }
    }, 
    "updateDate": {
      "xpath": {
        "url": "http://central.maven.org/maven2/${MVN_PATH}/maven-metadata.xml", 
        "xpath": "/metadata/versioning/lastUpdated/text()",
		"format": "%Y%m%d%H%M%S"
      }
    }
  }, 
  "license": "Apache License, Version 2.0"
}
