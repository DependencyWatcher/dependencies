{
  "name": "bootstrap",
  "pages": {
    "download": "http://getbootstrap.com/getting-started#download",
    "docs": "http://getbootstrap.com/${VERSION}/",
    "changelist": "https://github.com/twbs/bootstrap/releases/tag/v${VERSION}",
    "issues": "https://github.com/twbs/bootstrap/issues",
